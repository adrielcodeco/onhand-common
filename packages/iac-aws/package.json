    "@aws-cdk/aws-rds": "^1.85.0",
    "@aws-cdk/aws-s3": "^1.85.0",
    "@aws-cdk/cdk": "^0.36.1",
    "@aws-cdk/core": "^1.85.0",
    "@onhand/common-framework-aws": "^1.0.8",
    "@onhand/openapi": "^1.0.8",
    "archiver": "^5.2.0",
    "aws-cdk": "^1.85.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-winston": "^4.0.5",
    "fs-extra": "^9.1.0",
    "lambda-local": "^1.7.4",
    "lodash": "^4.17.20",
    "openapi-types": "^7.2.3",
    "reflect-metadata": "^0.1.13",
